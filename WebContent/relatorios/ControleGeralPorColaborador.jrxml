				<textFieldExpression class="java.lang.String"><![CDATA[(($F{dataCadastro}.after($P{dataInicio})) ? "*** " : "") +
$F{codCliente} + ($F{codPacote} == null ? "" : ("-" + $F{codPacote})) + ": " + $F{nomeProjeto} + ($F{codPacote} == null ? "" : (", Pacote: "+$F{codPacote}))]]></textFieldExpression>
			</textField>
			<textField>
